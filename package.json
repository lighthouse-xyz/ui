  "overrides": {
    "apollo-link-rest": {
      "graphql": "$graphql"
    }
  },
  "scripts": {
    "postinstall": "if [ -d \"extension\" ]; then cd extension && npm install; fi;",
    "prepare": "node ./prepare.js",
    "start:local": "REACT_APP_ENV=local GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-app-rewired --max_old_space_size=4096 start",
    "start": "REACT_APP_ENV=dev GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-app-rewired --max_old_space_size=4096 start",
    "start:prod": "REACT_APP_ENV=prod GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-app-rewired --max_old_space_size=4096 start",
    "serve": "serve -s build",
    "build:dev": "REACT_APP_ENV=dev GENERATE_SOURCEMAP=true react-app-rewired --max_old_space_size=4096 build",
    "build:prod": "REACT_APP_ENV=prod GENERATE_SOURCEMAP=true react-app-rewired --max_old_space_size=4096 build",
    "test": "REACT_APP_ENV=local react-app-rewired test --watchAll=false",
    "test:watch": "REACT_APP_ENV=local react-app-rewired test",
    "lint": "eslint --fix .",
    "pre-commit": "lint-staged",
    "generate:graphql": "graphql-codegen",
    "generate:graphql:local": "graphql-codegen --config ./codegen-local.yaml",
    "cy:open:local": "cypress open --env executionEnv=local",
    "cy:open:dev": "cypress open --env executionEnv=dev",
    "cy:open:prod": "cypress open --env executionEnv=prod",
    "cy:run:local": "cypress run --env executionEnv=local",
    "cy:run:dev": "cypress run --env executionEnv=dev",
    "cy:run:prod": "cypress run --env executionEnv=prod",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{tsx,ts}": [
      "npm run lint"
    ]
  },
  "jest": {
    "transform": {
      "node_modules/uint8arrays/cjs/src.+\\.(js|ts|cjs|mjs)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@src(|/.*)$": "<rootDir>/src/$1",
      "axios": "axios/dist/node/axios.cjs",
      "^uint8arrays$": "uint8arrays/cjs/src"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!uint8arrays/cjs/src/.*)"
    ],
    "setupFilesAfterEnv": [
      "./src/setup-test.ts"
    ],
    "clearMocks": true
  }
}
